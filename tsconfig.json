{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.js", "**/*.tsx", "**/*.jsx"]
} 