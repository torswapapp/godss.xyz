{
  "name": "flashloan-dashboard",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.26.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^6.4.3",
    "@sentry/react": "^7.0.0",
    "@vercel/analytics": "^1.4.1",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "compression": "^1.7.4",
    "cra-template": "1.2.0",
    "date-fns": "^2.30.0",
    "ethers": "^6.13.5",
    "framer-motion": "^12.4.1",
    "https-browserify": "^1.0.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-scripts": "5.0.1",
    "react-virtualized": "^9.22.3",
    "recharts": "^2.15.1",
    "source-map-explorer": "^2.5.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "web-vitals": "^4.2.4",
    "web3": "^4.16.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:production": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "npm run test:all && npm run test:coverage",
    "eject": "react-scripts eject",
    "predeploy": "node scripts/deployment-check.js",
    "vercel-build": "npm run predeploy && npm run build",
    "monitor:alerts": "node scripts/monitor-alerts.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "security-audit": "npm audit",
    "health-check": "node scripts/health-check.js",
    "backup": "node scripts/backup.js",
    "monitor": "node scripts/monitor-metrics.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "react-scripts test --testPathPattern=src/__tests__/unit",
    "test:integration": "react-scripts test --testPathPattern=src/__tests__/integration",
    "test:e2e": "cypress run",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test --watch",
    "rollback": "node scripts/rollback.js",
    "backup-state": "node scripts/backup-state.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "http": "stream-http",
    "https": "https-browserify",
    "zlib": "browserify-zlib",
    "url": "url/"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cypress": "^1.1.6",
    "cypress": "^13.17.0",
    "jest": "^29.7.0",
    "react-app-rewired": "^2.2.1"
  }
}
